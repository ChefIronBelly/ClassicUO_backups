clearsysmsg
clearignore 

wait 200
while findtype 3712|3648|3650|29833 backpack as found
    clearsysmsg 
    dclick found
    wait 200
    if not insysmsg "It appears to be locked."
        overhead "Already unlocked." 43
        ignore found
        wait 600
        continue
    endif
    
    #remove traps
    while not insysmsg "successfully"
        if insysmsg "devoid" 
            break
        endif
        wait 250
        gumpresponse 7
        wait 3500
    endwhile
    
    while not insysmsg "successfully"
        if insysmsg "devoid" 
            break
        endif
        wait 250
        gumpresponse 4
        wait 3500
    endwhile
    ignore found
endwhile