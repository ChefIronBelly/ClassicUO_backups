if not varexist store_id_items
    overhead "Select where you want to store your items" 88
    sysmsg "Jaseowns: Select where you want to store your items" 88
    setvar store_id_items
endif

if not find store_id_items ground -1 -1 2
    overhead "Can't find your storage container" 34
    overhead "You may need to reselect: store_id_items"
    stop
endif

overhead "Organize by finding unidentified items" 88
sysmsg "Jaseowns: Organize by finding unidentified items" 88

@setvar loot_stuck_check 0

foreach loot in huge_loot_items_list

    if loot_stuck_check = loot
        overhead "stuck or done!" 34
        break
    endif

    sysmsg loot 88
    
    while findtype loot backpack as item
        getlabel item descripition
        if unidentified in descripition
            overhead descripition 1489
            lift item
            wait 250
            drop store_id_items -1 -1 -1
         else 
            ignore item
            overhead descripition 34
            wait 250
        endif
    endwhile
    
    @setvar loot_stuck_check loot
    
endfor

overhead "All done!" 88
sysmsg "Jaseowns: All done organizing!" 88